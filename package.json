{
    "name": "atlas-ai-backend",
    "version": "1.0.0",
    "description": "Atlas AI Platform - Backend",
    "engines": {
        "node": ">=18.0.0"
    },
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc && mkdir -p dist/db/migrations && cp src/db/migrations/*.sql dist/db/migrations/",
        "start": "node dist/server.js",
        "migrate": "node dist/db/migrations/runner.js",
        "seed": "node dist/db/seeds/runner.js",
        "worker": "node dist/workers/index.js",
        "test": "jest"
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@notionhq/client": "^5.4.0",
        "axios": "^1.6.5",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "bullmq": "^5.1.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.10",
        "docxtemplater": "^3.43.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.1.5",
        "google-auth-library": "^9.6.3",
        "googleapis": "^131.0.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "joi": "^17.12.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.8",
        "openai": "^6.10.0",
        "pdfkit": "^0.14.0",
        "pg": "^8.11.3",
        "pizzip": "^3.1.6",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "ws": "^8.16.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/helmet": "^0.0.48",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.17",
        "@types/nodemailer": "^6.4.14",
        "@types/pdfkit": "^0.13.3",
        "@types/pg": "^8.16.0",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.10",
        "jest": "^29.7.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
